<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.metrodata.datamap" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <feature name="cordova-fix-blackscreen">
    <param name="android-package" value="cordova-fix-blackscreen"/>
  </feature>
  <feature name="HotCodePush">
    <param name="android-package" value="com.nordnetab.chcp.main.HotCodePushPlugin"/>
    <param name="onload" value="true"/>
  </feature>
  <feature name="File">
    <param name="android-package" value="org.apache.cordova.file.FileUtils"/>
    <param name="onload" value="true"/>
  </feature>
  <feature name="CordovaHttpPlugin">
    <param name="android-package" value="com.synconset.cordovahttp.CordovaHttpPlugin"/>
  </feature>
  <feature name="AMapPlugin">
    <param name="android-package" value="com.yxt.cordova.AMapPlugin"/>
  </feature>
  <feature name="BackbuttonPlugin">
    <param name="android-package" value="com.badrit.Backbutton.BackbuttonPlugin"/>
  </feature>
